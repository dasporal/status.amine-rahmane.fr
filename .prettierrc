{
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "parser": "react"
      }
    }
  ],
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "bracketSameLine": true,
  "singleAttributePerLine": true,
  "singleQuote": true
}